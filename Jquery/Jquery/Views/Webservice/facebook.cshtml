<script src="~/Scripts/jquery-3.3.1.js"></script>
@{
    ViewBag.Title = "facebook";
}

<h2>facebook WebService consume</h2>
<h2>Getting Web Service from Facebook, Real Time</h2>

<div id="fb-root"></div>
<script>
(function (d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) return;
        js = d.createElement(s); js.id = id;
        js.src = 'https://connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.12';
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));</script>

<div class="fb-page" data-href="https://www.facebook.com/facebook" data-tabs="timeline" 
     data-small-header="false" data-adapt-container-width="true" data-hide-cover="false" 
     data-show-facepile="true"><blockquote cite="https://www.facebook.com/facebook" 
     class="fb-xfbml-parse-ignore"><a href="https://www.facebook.com/facebook">Facebook</a></blockquote></div>



