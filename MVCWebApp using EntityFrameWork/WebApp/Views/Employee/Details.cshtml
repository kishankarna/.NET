@using WebApp.Models
@model Employee

@{
    ViewBag.Title = "Details";
}

<h2>Employee Details</h2>

<table>

    <tr><td>ID: @Model.ID </td></tr>
    <tr><td>Name: @Model.Name </td></tr>
    <tr><td>Gender: @Model.Gender </td></tr>
    <tr><td>City : @Model.City</td></tr>
    <tr><td>Department : @Model.Department_ID<td></tr>
</table>

@Html.ActionLink("Back to Employee List","Index","Employee",new {id=Model.Department_ID },null)